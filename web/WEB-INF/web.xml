<?xml version="1.0" encoding="UTF-8"?>
<web-app metadata-complete="false" version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>getDeals</servlet-name>
        <servlet-class>getDeals</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>recServlet</servlet-name>
        <servlet-class>com.thegoodlife2015.servlet.recServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>recordRating</servlet-name>
        <servlet-class>com.thegoodlife2015.servlet.recordRating</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>retrieveRating</servlet-name>
        <servlet-class>com.thegoodlife2015.servlet.retrieveRating</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>getOneRating</servlet-name>
        <servlet-class>com.thegoodlife2015.servlet.getOneRating</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>recServlet</servlet-name>
        <url-pattern>/recServlet/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>recordRating</servlet-name>
        <url-pattern>/recordRating</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>getDeals</servlet-name>
        <url-pattern>/getDeals</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>retrieveRating</servlet-name>
        <url-pattern>/retrieveRating/*</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>getOneRating</servlet-name>
        <url-pattern>/getOneRating</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
